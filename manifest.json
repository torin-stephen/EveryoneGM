{
  "manifest_version": 3,
  "name": "EveryoneGM - Chess.com",
  "version": "0.1.0",
  "icons": {
    "16": "icon_16.png",
    "19": "icon_19.png",
    "38": "icon_38.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
 "description": "This extension will make everyone a GM on chess.com (client-side).",
  "content_scripts": [
    {
      "matches": [
        "https://www.chess.com/play/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}